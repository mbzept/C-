#include <iostream>
using namespace std;
int main()
{
    float a,b,c;
    cout<<"a=";
    cin >> a;
    cout<<"b=";
    cin >>b;
    cout<<"c=";
    cin >>c;
    if (a+b >c || b+c>a || c+a>b ){
        if (a==b && b==c){
            cout<<"equilateral";
        }
        if (a==b || b==c || c==a){
            cout<<"isosceles";
        }
        else{
            cout<<"scalene";
        }
    }
    else{
        cout<<"can't form triangle";
    }
}
