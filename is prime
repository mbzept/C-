#include <iostream>
using namespace std;

int a = 10;

bool prime(int a) {
    if (a < 2) return false;
    int count = 0;
    for (int i = 2; i * i <= a; i++) {
        if (a % i == 0) {
            count++;
            if (count > 1) return false;
        }
    }
    return true;
}

int main() {
    if (prime(a)) {
        cout << "true" << endl;
    } else {
        cout << "false" << endl;
    }
    return 0;
}   
